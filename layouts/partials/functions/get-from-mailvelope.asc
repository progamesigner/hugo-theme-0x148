{{- $url := "https://keys.mailvelope.com/api/v1/key" -}}

{{- $field := 0 | index . | lower -}}
{{- $value := 1 | index . -}}

{{- $data := printf "%s?%s=%s" $url $field $value | getJSON -}}
{{- $key := $data.publicKeyArmored | htmlUnescape | htmlUnescape | safeHTML | strings.TrimRight "\n\r" -}}

{{- return $key -}}
